{
    "name": "laulamanapps/apple-passbook",
    "description": "Generate Apple Passbooks from PHP",
    "keywords": [
        "apple",
        "passbook",
        "php",
        "wallet",
        "event ticket",
        "coupon",
        "boarding pass",
        "store card",
        "iOS",
        "iPhone",
        "iPod Touch",
        "pkpass"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Laurens Laman",
            "email": "laurens.laman@llaman.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LauLamanApps\\ApplePassbook\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LauLamanApps\\ApplePassbook\\Tests\\": ["tests/"]
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "werkspot/enum": "^2.1 || ^3.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "ramsey/uuid": "^3 || ^4",
        "ext-exif": "*"
    },
    "suggest": {
        "ext-exif": "*",
        "laulamanapps/apple-passbook-bundle": "^2.0"
